{
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "semi": 0,
    "import/prefer-default-export": 0,
    "no-shadow": 0,
    "no-param-reassign": 0
  }
}
